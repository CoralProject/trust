{
  "dimensions": [
    {
      "name": "all",
      "label": "Anywhere",
      "value": "all"
    },
    {
      "name": "sections",
      "label": "In a section",
      "value": "sections",

      "querySet": "dimension_section_list",
      "labelField": "description",
      "valueField": "description"
    }
  ],
  "filters": [
