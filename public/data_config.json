      "name": "Comments",
      "category": "Activity",
      "field": "count",
      "template": "statistics.comments.<%= dimension %>.all.count",
      "description": "Total comments",
      "collection": "user_statistics",
      "type": "intRange"
    },
    {
      "name": "First comment _ days ago",
      "category": "Activity",
